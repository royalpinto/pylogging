{
  "name": "pylogging",
  "version": "1.0.0",
  "description": "Pythonic way of logging for JavaScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/royalpinto/pylogging.git"
  },
  "main": "lib/main.js",
  "scripts": {
    "test": "nodeunit test",
    "docs": "jsdoc2md lib/*.js > API.md"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^1.3.6",
    "nodeunit": "^0.9.1"
  },
  "author": "Royal Pinto <royalpinto@gmail.com>",
  "license": "MIT",
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-multiple-empty-lines": [
        2,
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "indent": [
        2,
        4,
        {
          "SwitchCase": 1
        }
      ]
    }
  },
  "dependencies": {
    "moment": "^2.13.0"
  }
}
