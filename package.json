{
  "name": "pylogging",
  "version": "1.0.0",
  "description": "Pythonic way of logging for JavaScript",
  "main": "lib/main.js",
  "devDependencies": {
    "nodeunit": "^0.9.1"
  },
  "author": "Royal Pinto <royalpinto@gmail.com>",
  "license": "MIT",
  "eslintConfig": {
    "extends": "google",
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "no-multiple-empty-lines": [
        2,
        {
          "max": 2,
          "maxEOF": 1
        }
      ],
      "indent": [
        2,
        4,
        {
          "SwitchCase": 1
        }
      ]
    }
  }
}
